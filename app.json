{
  "expo": {
    "name": "OwnInstagramClone",
    "slug": "OwnInstagramClone",
    "scheme": "InstagramClone",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/instagram.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/instagramLoading.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/instagram.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router"
    ]
  }
}